{
  "workbench.colorTheme": "Dracula Theme",
  "remote.SSH.remotePlatform": {
    "sstrax628164.destr.corpintra.net": "linux"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#ee337e77"
  },

  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.formatOnSave": true,
  "prettier.singleQuote": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.iconTheme": "vscode-icons",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false,
    "yaml": true
  },
  "terminal.integrated.shell.osx": "/bin/zsh",
  "plantuml.diagramsRoot": "docs/assets/src",
  "plantuml.exportOutDir": "docs/assets/out",
  "hediet.vscode-drawio.codeLinkActivated": false,
  "hediet.vscode-drawio.theme": "min",
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  // "xml.format.splitAttributesIndentSize": 2,
  "xml.format.splitAttributes": true,
  "java.debug.settings.showStaticVariables": false,
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "win-ca.inject": "append",
  "[kotlin]": {
    "editor.defaultFormatter": "fwcd.kotlin"
  },
  "prettier.bracketSpacing": false,
  "redhat.telemetry.enabled": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.rendererType": "dom",
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.fontFamily": "DroidSansMono Nerd Font",
  "i18n-ally.dirStructure": "auto",
  "i18n-ally.enabledFrameworks": ["react"],
  "vs-kubernetes": {
    "vs-kubernetes.crd-code-completion": "disabled"
  },
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "git.openRepositoryInParentFolders": "always",
  "sonarlint.connectedMode.connections.sonarqube": [],
  "editor.largeFileOptimizations": false,
  "[nginx]": {
    "editor.defaultFormatter": "raynigon.nginx-formatter"
  },
  "bridgeToKubernetes.disconnectAfterDebugging": false,
  "yaml.schemas": {
    "https://raw.githubusercontent.com/andrewsomething/app-platform-json-schema/refs/heads/main/app_spec.json": "*-app.yaml",
    "https://raw.githubusercontent.com/argoproj/argo-workflows/main/api/jsonschema/schema.json": "*.workflow.yaml",
    "/Users/tganste/yaml-schemas/telepresence-intercep-schema.json": "*intercept*.yaml",
    "https://raw.githubusercontent.com/argoproj/argo-events/master/api/jsonschema/schema.json": "*.(eventsource|sensor|eventbus|trigger).yaml"
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "hediet.vscode-drawio.resizeImages": null,
  "semanticdiff.defaultDiffViewer": true,
  "semanticdiff.closeOriginalTab": true,
  "files.associations": {
    "*.plmxml": "xml",
    "*.go": "go"
  },

  "editor.renderWhitespace": "none",
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },

  "go.goroot": "/opt/homebrew/opt/go/libexec",
  "workbench.sideBar.location": "right",
  "application.shellEnvironmentResolutionTimeout": 30,
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "go.toolsManagement.autoUpdate": true,
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  }
}
